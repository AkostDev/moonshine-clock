{
    "name": "akostdev/moonshine-clock-component",
    "description": "Analog clock for your admin panel and more",
    "keywords": ["clock", "moonshine", "compoenent"],
    "type": "library",
    "homepage": "https://github.com/AkostDev/moonshine-clock-component",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/AkostDev/moonshine-clock-component/issues",
        "source": "https://github.com/AkostDev/moonshine-clock-component"
    },
    "authors": [
        {
            "name": "Alex Kostylev",
            "email": "i@akost.dev",
            "homepage": "https://t.me/AkostDev"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "rector/rector": "^1.0",
        "moonshine/moonshine": "3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "AKostDev\\MoonShine\\": "src/"
        }
    },
    "conflict": {
        "moonshine/moonshine": "<3.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "rector": "vendor/bin/rector process --clear-cache",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AKostDev\\MoonShine\\Providers\\ClockServiceProvider"
            ]
        }
    }
}
